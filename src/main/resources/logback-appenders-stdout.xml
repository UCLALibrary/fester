<?xml version="1.0"?>
<included>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
      <level>DEBUG</level>
    </filter>
    <encoder>
      <pattern>avuongtest[%level] %logger{45}:%X{line} | %msg%n</pattern>
    </encoder>
  </appender>
</included>
